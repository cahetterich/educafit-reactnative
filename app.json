{
  "expo": {
    "name": "EducaFit",
    "slug": "educafit",
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/icon-192.png",
      "manifest": {
        "name": "EducaFit",
        "short_name": "EducaFit",
        "icons": [
          { "src": "./assets/icon-192.png", "sizes": "192x192", "type": "image/png" },
          { "src": "./assets/icon-512.png", "sizes": "512x512", "type": "image/png" },
          { "src": "./assets/maskable_icon.png", "sizes": "512x512", "type": "image/png", "purpose": "maskable" }
        ],
        "theme_color": "#1F6FEB",
        "background_color": "#FFFFFF",
        "display": "standalone"
      }
    }
  }
}